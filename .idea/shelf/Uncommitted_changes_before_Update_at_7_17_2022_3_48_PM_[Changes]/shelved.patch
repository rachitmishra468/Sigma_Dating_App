+                if (charString.isEmpty()) photosListFiltered =  ArrayList() else {
+                    val filteredList = ArrayList<UniversityList>()
+                    for ( i in photosList ){
+                        if (i.name.toUpperCase().trim().contains(constraint.toString().toUpperCase().trim())) {
+                            filteredList.add(i);
+                        }
+                    }
+                    photosListFiltered=filteredList
+
+                   /* photosList
